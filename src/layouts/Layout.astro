---
import '../styles/global.scss';
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang='en'>
	<head>
		<meta charset='UTF-8' />
		<meta name='description' content='3D printing services' />
		<meta name='viewport' content='width=device-width' />
		<link rel='icon' type='image/svg+xml' href='/images/OIG_trim.png' />
		<meta name='generator' content={Astro.generator} />

		<link rel='stylesheet' href='https://storage.googleapis.com/code.getmdl.io/1.0.6/material.blue-orange.min.css' />

		<title>{title} | PolarTec</title>
	</head>
	<body>
		<slot />
	</body>
</html>
